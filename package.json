{
  "name": "ipcommunicationlayer",
  "version": "1.0.0",
  "description": "A generated IBM Cloud application",
  "private": true,
  "engines": {
    "node": "10.15.3"
  },
  "config": {
    "entrypoint": "src/server.js"
  },
  "scripts": {
    "start": "node $npm_package_config_entrypoint",
    "debug": "node --inspect=0.0.0.0:9229 $npm_package_config_entrypoint",
    "test": "nyc mocha --exit",
    "dev": "nodemon $npm_package_config_entrypoint",
    "start-grpc-test" : "node playground/test-grpc.js | pino-pretty"
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal"
    }
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.0",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "config": "^3.3.1",
    "crypto": "^1.0.1",
    "fastify": "^2.3.0",
    "grpc": "^1.20.3",
    "grpc-kit": "^0.2.0",
    "node-rsa": "^1.0.5",
    "pino": "^5.12.3",
    "pino-pretty": "^3.0.0",
    "pino-redis": "^1.0.3",
    "randomstring": "^1.1.5",
    "redis": "^2.8.0",
    "requestretry": "^4.0.0",
    "simple-hashtable": "^1.0.0",
    "socket.io": "^2.2.0",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0",
    "socketio-auth": "^0.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^1.1.3",
    "mocha": "^6.1.4",
    "nodemon": "^1.12.1",
    "nyc": "^14.1.0",
    "prompt-confirm": "^1.2.0",
    "request": "^2.82.0"
  }
}
